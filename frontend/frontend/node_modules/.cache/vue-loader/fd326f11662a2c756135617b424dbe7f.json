urcesContent":["<template>\n  <div class=\"jumbotron vertical-center\">\n    <div class=\"container\">\n      <link\n        rel=\"stylesheet\"\n        href=\"https://cdn.jsdelivr.net/npm/bootswatch@4.5.2/dist/journal/bootstrap.min.css\"\n        integrity=\"sha384-QDSPDoVOoSWz2ypaRUidLmLYl4RyoBWI44iA5agn6jHegBxZkNqgm2eHb6yZ5bYs\"\n        crossorigin=\"anonymous\"\n      />\n      <div class=\"row\">\n        <div class=\"col-sm-12\">\n          <h1\n            class=\"text-center bg-primary text-white\"\n            style=\"border-radius: 10px\"\n          >\n            Games Library üïπÔ∏è\n          </h1>\n          <hr />\n          <br />\n\n          <b-alert variant=\"success\" v-if=\"showMessage\" show>\n            {{ message }}\n          </b-alert>\n\n          <button\n            type=\"button\"\n            class=\"btn btn-success btn-sm\"\n            v-b-modal.game-modal\n          >\n            Add Game\n          </button>\n          <br /><br />\n          <table class=\"table table-hover\">\n            <thead>\n              <tr>\n                <th scope=\"col\">Title</th>\n                <th scope=\"col\">Genre</th>\n                <th scope=\"col\">Progress</th>\n                <th scope=\"col\">Completed?</th>\n                <th scope=\"col\">Actions</th>\n              </tr>\n            </thead>\n            <tbody>\n              <tr v-for=\"(game, index) in games\" :key=\"index\">\n                <td>{{ game.title }}</td>\n                <td>{{ game.genre }}</td>\n                <td>{{ game.progress }}</td>\n                <td>\n                  <span v-if=\"game.played\">Yes</span>\n                  <span v-else>No</span>\n                </td>\n                <td>\n                  <div class=\"btn-group\" role=\"group\">\n                    <button\n                      type=\"button\"\n                      class=\"btn btn-info btn-sm\"\n                      v-b-modal.game-update-modal\n                      @click=\"editGame(game)\"\n                    >\n                      Update\n                    </button>\n                    <button\n                      type=\"button\"\n                      class=\"btn btn-danger btn-sm\"\n                      @click=\"deleteGame(game)\"\n                    >\n                      Delete\n                    </button>\n                  </div>\n                </td>\n              </tr>\n            </tbody>\n          </table>\n          <Footer\n            class=\"bg-primary text-white text-center\"\n            style=\"border-radius: 10px\"\n            >Enjoy!!!</Footer\n          >\n        </div>\n      </div>\n\n      <b-modal\n        ref=\"addGameModal\"\n        id=\"game-modal\"\n        title=\"Add a new game\"\n        hide-backdrop\n        hide-footer\n      >\n        <b-form @submit=\"onSubmit\" @reset=\"onReset\" class=\"w-100\">\n          <b-form-group\n            id=\"form-title-group\"\n            label=\"Title:\"\n            label-for=\"form-title-input\"\n          >\n            <b-form-input\n              id=\"form-title-input\"\n              type=\"text\"\n              v-model=\"addGameForm.title\"\n              required\n              placeholder=\"Enter title\"\n            >\n            </b-form-input>\n          </b-form-group>\n\n          <b-form-group\n            id=\"form-genre-group\"\n            label=\"Genre:\"\n            label-for=\"form-genre-input\"\n          >\n            <b-form-input\n              id=\"form-genre-input\"\n              type=\"text\"\n              v-model=\"addGameForm.genre\"\n              required\n              placeholder=\"Enter genre\"\n            >\n            </b-form-input>\n          </b-form-group>\n\n          <b-form-group\n            id=\"form-progress-group\"\n            label=\"Progress:\"\n            label-for=\"form-progress-input\"\n          >\n            <b-form-input\n              id=\"form-progress-input\"\n              type=\"text\"\n              v-model=\"addGameForm.progress\"\n              required\n              placeholder=\"Enter progress in %\"\n            >\n            </b-form-input>\n          </b-form-group>\n\n          <b-form-group id=\"form-played-group\">\n            <b-form-checkbox-group\n              v-model=\"addGameForm.played\"\n              id=\"form-checks\"\n            >\n              <b-form-checkbox value=\"true\">Played?</b-form-checkbox>\n            </b-form-checkbox-group>\n          </b-form-group>\n\n          <b-button type=\"submit\" variant=\"outline-info\">Submit</b-button>\n          <b-button type=\"reset\" variant=\"outline-danger\">Reset</b-button>\n        </b-form>\n      </b-modal>\n\n      <b-modal\n        ref=\"editGameModal\"\n        id=\"game-update-modal\"\n        title=\"Update\"\n        hide-backdrop\n        hide-footer\n      >\n        <b-form @submit=\"onSubmitUpdate\" @reset=\"onResetUpdate\" class=\"w-100\">\n          <b-form-group\n            id=\"form-title-edit-group\"\n            label=\"Title:\"\n            label-for=\"form-title-edit-input\"\n          >\n            <b-form-input\n              id=\"form-title-edit-input\"\n              type=\"text\"\n              v-model=\"editForm.title\"\n              required\n              placeholder=\"Enter title\"\n            >\n            </b-form-input>\n          </b-form-group>\n\n          <b-form-group\n            id=\"form-genre-edit-group\"\n            label=\"Genre:\"\n            label-for=\"form-genre-edit-input\"\n          >\n            <b-form-input\n              id=\"form-genre-edit-input\"\n              type=\"text\"\n              v-model=\"editForm.genre\"\n              required\n              placeholder=\"Enter genre\"\n            >\n            </b-form-input>\n          </b-form-group>\n\n          <b-form-group\n            id=\"form-progress-edit-group\"\n            label=\"Progress:\"\n            label-for=\"form-progress-edit-input\"\n          >\n            <b-form-input\n              id=\"form-progress-edit-input\"\n              type=\"text\"\n              v-model=\"editForm.progress\"\n              required\n              placeholder=\"Enter progress in %\"\n            >\n            </b-form-input>\n          </b-form-group>\n\n          <b-form-group id=\"form-played-edit-group\">\n            <b-form-checkbox-group v-model=\"editForm.played\" id=\"form-checks\">\n              <b-form-checkbox value=\"true\">Played?</b-form-checkbox>\n            </b-form-checkbox-group>\n          </b-form-group>\n\n          <b-button-group>\n            <b-button type=\"submit\" variant=\"outline-info\">Update</b-button>\n            <b-button type=\"reset\" variant=\"outline-danger\">Cancel</b-button>\n          </b-button-group>\n        </b-form>\n      </b-modal>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  data() {\n    return {\n      games: [],\n      addGameForm: {\n        title: \"\",\n        genre: \"\",\n        progress: \"\",\n        played: [],\n      },\n      editForm: {\n        id: \"\",\n        title: \"\",\n        genre: \"\",\n        progress: \"\",\n        played: [],\n      },\n    };\n  },\n\n  message: \"\",\n\n  methods: {\n    getGames() {\n      const path = \"http://localhost:5000/\";\n      axios\n        .get(path)\n        .then((res) => {\n          this.games = res.data.games;\n        })\n        .catch((error) => {\n          console.error(error);\n        });\n    },\n\n    addGame(payload) {\n      const path = \"http://localhost:5000/\";\n      axios\n        .post(path, payload)\n        .then(() => {\n          this.getGames();\n\n          this.message = \"Game added üïπÔ∏è !\";\n\n          this.showMessage = true;\n        })\n        .catch((error) => {\n          console.log(error);\n          this.getGames();\n        });\n    },\n\n    initForm() {\n      this.addGameForm.title = \"\";\n      this.addGameForm.genre = \"\";\n      this.addGameForm.progress = \"\";\n      this.addGameForm.played = [];\n      this.editForm.id = \"\";\n      this.editForm.title = \"\";\n      this.editForm.genre = \"\";\n      this.editForm.progress = \"\";\n      this.editForm.played = [];\n    },\n\n    onSubmit(e) {\n      e.preventDefault();\n      this.$refs.addGameModal.hide();\n      let played = false;\n      if (this.addGameForm.played[0]) played = true;\n      const payload = {\n        title: this.addGameForm.title,\n        genre: this.addGameForm.genre,\n        progress: this.addGameForm.progress,\n        played,\n      };\n      this.addGame(payload);\n      this.initForm();\n    },\n\n    onSubmitUpdate(e) {\n      e.preventDefault();\n      this.$refs.editGameModal.hide();\n      let played = false;\n      if (this.editForm.played[0]) played = true;\n      const payload = {\n        title: this.editForm.title,\n        genre: this.editForm.genre,\n        progress: this.\n        played,\n      };\n      this.updateGame(payload, this.editForm.id);\n    },\n\n    onReset(e) {\n      e.preventDefault();\n      this.$refs.addGameModal.hide();\n      this.initForm();\n    },\n\n    updateGame(payload, gameID) {\n      const path = `http://localhost:5000/${gameID}`;\n      axios\n        .put(path, payload)\n        .then(() => {\n          this.getGames();\n          this.message = \"Game updated ‚öôÔ∏è!\";\n          this.showMessage = true;\n        })\n        .catch((error) => {\n          console.error(error);\n          this.getGames();\n        });\n    },\n\n    editGame(game) {\n      this.editForm = game;\n    },\n\n    onResetUpdate(e) {\n      e.preventDefault();\n      this.$refs.editGameModal.hide();\n      this.initForm();\n      this.getGames();\n    },\n\n    removeGame(gameID) {\n      const path = `http://localhost:5000/${gameID}`;\n      axios\n        .delete(path)\n        .then(() => {\n          this.getGames();\n          this.message = \"Game Removed üóëÔ∏è!\";\n          this.showMessage = true;\n        })\n        .catch((error) => {\n          console.error(error);\n          this.getGames();\n        });\n    },\n\n    deleteGame(game) {\n      this.removeGame(game.id);\n    },\n  },\n  created() {\n    this.getGames();\n  },\n};\n</script>\n"]}]}